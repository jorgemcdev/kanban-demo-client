{
    "extends": "airbnb",
    "parserOptions":{
        "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "experimentalDecorators": true
        }
    },    
    "plugins": [
        "react"
    ],
    "rules":{
        "comma-dangle": 0,
        "no-console": 1,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "experimental-decorators": 0,
        "new-cap": 0
    }
}